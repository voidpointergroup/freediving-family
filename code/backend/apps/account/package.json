{
    "name": "account",
    "version": "1.0.0",
    "main": "",
    "author": "",
    "license": "MIT",
    "type": "module",
    "description": "",
    "scripts": {
        "codegen:protoc:bus": "rm -rf ./src/__generated__/proto/bus && mkdir -p ./src/__generated__/proto/bus/ts && protoc -I ../../libs --plugin ./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out ./src/__generated__/proto/bus/ts ../../libs/bus/*.proto",
        "codegen:graphql": "npx graphql-codegen --config ./codegen.yml",
        "codegen": "npm-run-all codegen:**",
        "build:graphql": "npm-run-all codegen:** && npx ncc build -m src/graphql/index.ts -o ./dist/graphql --target es2020",
        "build:worker": "npm-run-all codegen:** && npx ncc build src/worker/index.ts -o ./dist/worker --target es2020",
        "build": "npm-run-all build:**"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.16.1",
        "@graphql-codegen/typescript": "^2.8.5",
        "@graphql-codegen/typescript-resolvers": "^2.7.10",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.11.17",
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.47.0",
        "@vercel/ncc": "^0.36.0",
        "eslint": "^8.30.0",
        "jest": "^29.4.2",
        "npm-run-all": "^4.1.5",
        "ts-proto": "^1.136.1",
        "ttypescript": "^1.5.15",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@apollo/server": "^4.3.0",
        "@apollo/subgraph": "^2.2.2",
        "body-parser": "^1.20.1",
        "cross-fetch": "^3.1.5",
        "express": "^4.18.2",
        "graphql": "^16.6.0",
        "graphql-tag": "^2.12.6",
        "mongodb": "^4.13.0",
        "nats": "^2.10.2",
        "ulid": "^2.3.0",
        "yaml": "^2.2.0"
    }
}
